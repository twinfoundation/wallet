			]
		},
		"packages/wallet-connector-iota": {
			"package-name": "wallet-connector-iota",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/index.ts"
			]
		},
		"apps/wallet-cli": {
			"package-name": "wallet-cli",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/cli.ts",
				"tests/cli.spec.ts",
				"src/index.ts"
			]
		}
	},
	"plugins": [
		"node-workspace",
